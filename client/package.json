{
	"name": "financio",
	"version": "0.0.0",
	"description": "Financio",
	"private": true,
	"main": "index.html",
	"scripts": {
		"setup": "yarn install --pure-lockfile",
		"update": "yarn install",
		"rsync_www": "mkdir -p dist; rsync -r --delete --exclude=\"config.js\" www/ dist/",
		"build": "yarn rsync_www; webpack --config webpack.dev.js",
		"build-production": "yarn rsync_www; webpack --config webpack.prod.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fiddleplum/financio.git"
	},
	"author": "Stephen Hurley",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/fiddleplum/financio/issues"
	},
	"homepage": "https://github.com/fiddleplum/financio",
	"dependencies": {
		"fontmetrics": "latest"
	},
	"devDependencies": {
		"eslint": "latest",
		"eslint-config-standard": "latest",
		"eslint-plugin-import": "latest",
		"eslint-plugin-node": "latest",
		"eslint-plugin-promise": "latest",
		"eslint-plugin-standard": "latest",
		"webpack": "latest",
		"webpack-cli": "latest",
		"webpack-dev-server": "latest",
		"webpack-merge": "latest",
		"svg-inline-loader": "latest"
	}
}
